<header id="top-header">
  <span class="siteName">
    <h1>CSE Motors</h1>
  </span>
  <div id="tools">
    <% if (user.account_type ==='Client') { %>
      <a title="My Account" href="/account">My Account</a>
      
      <% if (user.account_type === 'Employee' || user.account_type === 'Admin') { %>
        <a href="/inventory/management">Manage Inventory</a>
      <% } %>
      
      <a title="Log Out" href="/account/logout">Log Out</a>
    <% } else { %>
      <a title="Click to log in" href="/account/login">My Account</a>
    <% } %>
  </div>
</header>